<!DOCTYPE html>t
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MSJ_Enterprises</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MSJ_Enterprises.styles.css" asp-append-version="true" />
</head>
<body>
    <header class="header">
        <nav style="background-color: black;" class="navbar navbar-expand-sm navbar-toggleable-sm navbar-ligh border-bottom box-shadow mb-3">
            <div class="container-fluid d-flex justify-content-center">
               <h1 id="heading" style="color:white; font-family:'Times New Roman', Times, serif; font-size:50px">MSJ Enterprises</h1>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
